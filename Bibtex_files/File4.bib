@article{Douglas1996,
abstract = {A toxicity identification evaluation (TIE) was conducted on the effluent from a petrochemical plant which discharges into an estuary. The effluent had been consistently toxic to mysid shrimp (Mysidopsis bahia) but not toxic to sheepshead minnows (Cyprinodon variegatus). Phase I effluent toxicity characterization tests revealed that treatment of the effluent with a cation exchange resin (Amberlite{\textregistered} IR-120 Plus) was partially effective at reducing, but not removing, toxicity. Phase II characterization tests revealed that four cartons varied with toxicity: Ca and Sn were positively correlated with increasing toxicity; Mg and K were negatively correlated with increasing toxicity. Toxicity tests with SnCl2 revealed that the toxicity threshold for Sn was far above the concentrations present in the raw effluent. Reduction of Ca was shown to result in a significant improvement in survival, but some toxicity still remained. Further augmentation of the treated effluent with CaCl2, MgCl2, and KBr to restore the concentrations of Ca, Mg, K, and Br to natural seawater concentrations resulted in survival of all exposed organisms. Repeated success of this treatment regime on additional samples of the effluent as well as 'mock effluent' studies confirmed that ion imbalance was the sole source of toxicity in this effluent. Process source water composition and essential ion balance are discussed as important components of marine effluent TIEs.},
author = {Douglas, W. S. and Grasso, S. S. and Hutton, D. G. and Schroeder, K. R.},
journal = {Archives of Environmental Contamination and Toxicology},
number = {3},
pages = {426--432},
pmid = {8854838},
publisher = {Springer New York LLC},
title = {{Ionic imbalance as a source of toxicity in an estuarine effluent}},
volume = {31},
year = {1996}
}
