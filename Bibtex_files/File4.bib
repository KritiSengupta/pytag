@article{Staples1998,
abstract = {Ethylene glycol ethers and acetates are used as intermediates, solvents, and plasticizers. They primarily enter the environment from manufacturing effluents and emissions and during their use in commercial products. Therefore, an examination of their ultimate fate and toxicity, as well as their potential for exposure, was performed. Overall, these data show that ethylene glycol ethers and acetates are not persistent in the environment, are not bioaccumulative, are generally classified by U.S. Environmental Protection Agency (EPA) procedures as 'practically non-toxic' to aquatic organisms based on acute toxicity, and that conservatively calculated exposures are mostly below concentrations of concern for chronic risks to aquatic life.},
author = {Staples, Charles A. and Boatman, Rodney J. and Cano, Manuel L.},
journal = {Chemosphere},
month = {mar},
number = {7},
pages = {1585--1613},
pmid = {9503579},
title = {{Ethylene glycol ethers: An environmental risk assessment}},
volume = {36},
year = {1998}
}
