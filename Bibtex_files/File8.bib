journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
keywords = {Endocrine-disrupting chemicals,Landfill leachate,Marine plastic debris,Microplastics,Persistent organic pollutants,Plastic resin pellet},
month = {jul},
